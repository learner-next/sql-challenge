{
  "name": "sql-challenge",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.0.22",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check": "tsc --incremental --noEmit && rm -f tsconfig.tsbuildinfo",
    "lint": "eslint . --cache --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --cache --write \"src/**/*.{ts,tsx,json,css,scss,md}\"",
    "preview": "vite preview",
    "postinstall": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run lint && bun check && bun lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,json,md,css,scss}": [
      "prettier --cache --write"
    ],
    "*.ts?(x)": [
      "eslint --cache --fix",
      "prettier --cache --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "@bytemd/plugin-gemoji": "^1.21.0",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/react": "^1.21.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-router": "beta",
    "alasql": "^4.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "github-markdown-css": "^5.5.0",
    "jotai": "^2.6.1",
    "lucide-react": "^0.316.0",
    "monaco-editor": "^0.45.0",
    "monaco-editor-component": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.0",
    "sql-formatter": "^15.0.2",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "bun-types": "latest",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3"
  }
}
