{
  "name": "sql-challenge",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.1.3",
  "author": "talljack <yugang.cao12@gmail.com> (https://github.com/talljack)",
  "funding": "https://github.com/sponsors/talljack",
  "homepage": "https://github.com/learner-next/sql-challenge",
  "bugs": "https://github.com/learner-next/sql-challenge/issues",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check": "tsc --incremental --noEmit && rm -f tsconfig.tsbuildinfo",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "format": "prettier --cache --write \"src/**/*.{ts,tsx,json,css,scss,md}\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@bytemd/plugin-gemoji": "^1.21.0",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/react": "^1.21.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-router": "beta",
    "alasql": "^4.3.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "github-markdown-css": "^5.5.1",
    "jotai": "^2.8.0",
    "lucide-react": "^0.379.0",
    "monaco-editor": "^0.48.0",
    "monaco-editor-component": "^0.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.1.0",
    "sql-formatter": "^15.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "bun-types": "latest",
    "chalk": "^5.3.0",
    "eslint": "^9.1.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.3",
    "talljack-eslint-config": "^0.2.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "bun check && bun lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  }
}
