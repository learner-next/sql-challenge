{
  "name": "sql-challenge",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.0.32",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check": "tsc --incremental --noEmit && rm -f tsconfig.tsbuildinfo",
    "lint": "eslint . --cache --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --cache --write \"src/**/*.{ts,tsx,json,css,scss,md}\"",
    "preview": "vite preview",
    "postinstall": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run lint && bun check && bun lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,json,md,css,scss}": [
      "prettier --cache --write"
    ],
    "*.ts?(x)": [
      "eslint --cache --fix",
      "prettier --cache --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "@bytemd/plugin-gemoji": "^1.21.0",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/react": "^1.21.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-router": "beta",
    "alasql": "^4.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "github-markdown-css": "^5.5.1",
    "jotai": "^2.7.1",
    "lucide-react": "^0.365.0",
    "monaco-editor": "^0.47.0",
    "monaco-editor-component": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "sql-formatter": "^15.3.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.18",
    "bun-types": "latest",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.36",
    "postcss-import": "^16.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "simple-git-hooks": "^2.11.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
